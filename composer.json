{
    "require": {
        "php": ">=8.1",
        "ext-ds": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard": "^11.0",
        "vimeo/psalm": "^4.24",
        "psalm/plugin-phpunit": "^0.17.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
